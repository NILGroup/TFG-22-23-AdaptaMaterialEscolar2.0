{
	"printWidth": 120,
	"tabWidth": 4,
	"useTabs": true,
	"semi": true,
	"singleQuote": false,
	"quoteProps": "as-needed",
	"trailingComma": "es5",
	"bracketSpacing": true,
	"bracketSameLine": false,
	"arrowParens": "always",
	"plugins": ["prettier-plugin-organize-imports"],
	"pluginSearchDirs": false,
	"organizeImportsSkipDestructiveCodeActions": true
}
